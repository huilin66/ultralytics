# build
docker build -t mmdet:v1.6 .
docker build -t mmdet:v2.3 .


# login
docker login --username=18271899387 registry.cn-hangzhou.aliyuncs.com


# link
docker tag mmdet:v1.6 registry.cn-hangzhou.aliyuncs.com/test_spacename/mmdet:v1.6
docker tag mmdet:v2.3 registry.cn-hangzhou.aliyuncs.com/test_spacename/mmdet:v2.3

# push
docker push registry.cn-hangzhou.aliyuncs.com/test_spacename/mmdet:v1.6
docker push registry.cn-hangzhou.aliyuncs.com/test_spacename/mmdet:v2.3

# run
docker run --gpus all --rm -v /E/data/tp/multi_modal_airplane_train/infer_img:/input_path -v /E/data/tp/multi_modal_airplane_train/infer_result:/output_path mmdet:v1.3
